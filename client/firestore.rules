rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if
          request.time < timestamp.date(2021, 6, 29);
    }
  }
}

rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read: if isSignedIn();
    }

    match /{document=**} {
      allow read, write: if isSignedIn();
    }

    function isSignedIn() {
      return request.auth.uid != null;
    }

  }
}
